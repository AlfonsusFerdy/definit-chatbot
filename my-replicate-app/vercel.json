{
  "functions": {
    "api/chat.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/api/chat.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REPLICATE_API_TOKEN": "@replicate_api_token"
  },
  "build": {
    "env": {
      "REPLICATE_API_TOKEN": "@replicate_api_token"
    }
  }
}
